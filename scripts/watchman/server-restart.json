[
    "trigger",
    "/app",
    {
        "name": "server-restart",
        "expression": [
            "name",
            [
                "vite.config.mjs",
                "src/app.html",
                "svelte.config.js",
                "tsconfig.json"
            ],
            "wholename"
        ],
        "command": [
            "/app/scripts/develop.sh",
            "serve"
        ],
        "append_files": false
    }
]
